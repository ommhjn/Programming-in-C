#include <stdio.h>
int main()
{
	int n1,n2;
	printf("Enter two numbers: ");
    scanf("%d",&n1);
    scanf("%d",&n2);

    printf("%d < %d: %s\n",n1,n2,(n1 <n2) ? "True" : "False");
    printf("%d <= %d: %s\n",n1,n2,(n1 <= n2) ? "True" : "False");
	printf("%d > %d: %s\n",n1,n2,(n1 > n2) ? "True" : "False");
	printf("%d >= %d: %s\n",n1,n2,(n1 >= n2) ? "True" : "False");
	printf("%d == %d: %s\n",n1,n2,(n1 == n2) ? "True" : "False");
    printf("%d != %d: %s\n",n1,n2,(n1 != n2) ? "True" : "False");
return 0;
}
