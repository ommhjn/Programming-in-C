#include <stdio.h>

// Enum to track the type of data stored in the union
enum Type {
    INTEGER,
    FLOATING_POINT
}; 

// Union definition
union Data {
	int integer;
	float floating_point;
};
struct Variable {
enum Type type;
union Data value;
};
// Structure definition to track the type of data stored in the union


int main() {
    // Declare two structure variables
    struct Variable variable1, variable2;

    // Input for variable1 (integer)
    scanf("%d", &variable1.value.integer);
    variable1.type = INTEGER;

    // Clear the input buffer
    int c;
    while ((c = getchar()) != '\n' && c != EOF);

    // Input for variable2 (float)
    scanf("%f", &variable2.value.floating_point);
    variable2.type = FLOATING_POINT;

    // Print variable1
    if (variable1.type == INTEGER) {
        printf("%d\n", variable1.value.integer);
    } else {
        printf("%f\n", variable1.value.floating_point);
    }

    // Print variable2
    if (variable2.type == INTEGER) {
        printf("%d\n", variable2.value.integer);
    } else {
        printf("%.1f\n", variable2.value.floating_point);
    }

    return 0;
}
